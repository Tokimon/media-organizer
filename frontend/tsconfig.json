{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true
  },

  "exclude": ["./src/lib/wailsjs"],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
